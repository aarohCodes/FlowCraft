# Video Meeting API Configuration

# Zoom
VITE_ZOOM_CLIENT_ID=your_zoom_client_id
VITE_ZOOM_CLIENT_SECRET=your_zoom_client_secret
VITE_ZOOM_WEBHOOK_SECRET=your_webhook_secret_token

# Google Meet & Gmail
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Discord
VITE_DISCORD_CLIENT_ID=your_discord_client_id
VITE_DISCORD_CLIENT_SECRET=your_discord_client_secret

# Microsoft Teams
VITE_TEAMS_CLIENT_ID=your_teams_client_id
VITE_TEAMS_CLIENT_SECRET=your_teams_client_secret

# Optional: Redirect URIs (auto-detected in development)
VITE_ZOOM_REDIRECT_URI=http://localhost:5173/video-meeting/callback/zoom
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/video-meeting/callback/google-meet
VITE_DISCORD_REDIRECT_URI=http://localhost:5173/video-meeting/callback/discord
VITE_TEAMS_REDIRECT_URI=http://localhost:5173/video-meeting/callback/microsoft-teams
VITE_GMAIL_REDIRECT_URI=http://localhost:5173/gmail/callback